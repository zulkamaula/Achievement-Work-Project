<div class="modal fade" id="detailCustomer" tabindex="-1" role="dialog" aria-labelledby="detailCustomerTitle" aria-hidden="true" data-keyboard="false" data-backdrop="static">
    <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header" style="border:none;">
                <h5 class="modal-title" id="exampleModalLongTitle">
                    Detail (No Customer : <span id="noCustomerOnHand"></span>)
                </h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" onclick="window.location.reload();">
                    <span aria-hidden="true"> &times; </span>
                </button>
            </div>
            <div class="modal-body text-center center" style="border:none;">
                <div id="onhandLoading">
                    <div class="spinner-grow text-primary" role="status">
                        <span class="sr-only">Loading...</span>
                    </div>
                    <div class="spinner-grow text-secondary" role="status">
                        <span class="sr-only">Loading...</span>
                    </div>
                    <div class="spinner-grow text-success" role="status">
                        <span class="sr-only">Loading...</span>
                    </div>
                    <div class="spinner-grow text-danger" role="status">
                        <span class="sr-only">Loading...</span>
                    </div>
                    <div class="spinner-grow text-warning" role="status">
                        <span class="sr-only">Loading...</span>
                    </div>
                    <div class="spinner-grow text-info" role="status">
                        <span class="sr-only">Loading...</span>
                    </div>
                    <div class="spinner-grow text-dark" role="status">
                        <span class="sr-only">Loading...</span>
                    </div>
                </div>
                <div id="customerOnHandUpload" style="display:none;">
                    <table id="onhandTable" class="table table-sm" style="font-size:12px;">
                        <thead>
                            <tr>
                                <th scope="col"> Nama Customer </th>
                                <th scope="col"> Telp </th>
                                <th scope="col"> E-mail </th>
                                <th scope="col"> Plat Kendaraan </th>
                            </tr>
                        </thead>
                    </table>
                </div>
                <small>
                    (!) Halaman ini akan ditutup secara otomatis
                </small>
            </div>
        </div>
    </div>
</div>